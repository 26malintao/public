<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zl.dao.IStateDao" >
	<!-- 根据Id级联 -->
	<select id="findStateById" resultType="com.zl.pojo.State" parameterType="java.lang.Integer">
		select * from state where stateid=#{stateid} 
	</select>
	<!-- 查询所有厂商 -->
	<select id="findAllState" resultType="com.zl.pojo.State">
		select * from state
	</select>
</mapper>