<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zl.dao.IUsersDao" >
	<select id="IsLogin" resultType="com.zl.pojo.User" parameterType="com.zl.pojo.User">
		select userid,loginname,loginpwd,realname from users where loginname=#{loginname} and loginpwd=#{loginpwd}
	</select>


</mapper>